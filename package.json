{
  "name": "app2",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "main": "electron/main.js",
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "lint": "eslint .",
    "preview": "vite preview",
    "build:react": "vite build",
    "pack": "npm run build:react && electron-builder --dir",
    "dist": "npm run build:react && electron-builder"
  },
  "dependencies": {
    "bootstrap": "^5.3.8",
    "datatables.net-bs5": "^2.3.4",
    "datatables.net-dt": "^2.3.4",
    "datatables.net-react": "^1.0.1",
    "electron-updater": "^6.6.2",
    "react": "^19.1.1",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.1",
    "react-router-dom": "^7.9.6",
    "sqlite3": "^5.1.7",
    "sweetalert2": "^11.26.3",
    "uuid": "^13.0.0"
  },
  "build": {
    "appId": "com.devco.caedro",
    "productName": "Caedro ERP",
    "files": [
      "dist/**/*",
      "electron/**/*",
      "package.json"
    ],
    "publish": [
      {
        "provider": "github",
        "owner": "sebaslopezg",
        "repo": "electron-vite-proyect"
      }
    ],
    "asar": false,
    "extraResources": [
      {
        "from": "electron/inventario.db",
        "to": "inventario.db"
      }
    ],
    "directories": {
      "output": "release"
    },
    "win": {
      "target": "nsis",
      "icon": "assets/favicon.png"
    },
    "mac": {
      "target": "dmg",
      "icon": "assets/favicon.icns"
    }
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.3",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.1",
    "cross-env": "^10.0.0",
    "electron": "^38.6.0",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.4.0",
    "vite": "^7.1.7",
    "wait-on": "^9.0.3"
  }
}
